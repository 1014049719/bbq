#import <Foundation/Foundation.h>

@interface RelativeModel : NSObject

/// 亲友ID
@property (copy, nonatomic) NSString *uid;
/// 亲友登陆名
@property (copy, nonatomic) NSString *userName;
/// 头像URL
@property (copy, nonatomic) NSString *avatar;
/// 昵称
@property (copy, nonatomic) NSString *nickName;
/// 访问次数
@property (copy, nonatomic) NSString *visit_count;
/// 最后访问时间
@property (assign, nonatomic) int visit_dateline;
/// 关系ID
@property (copy, nonatomic) NSString *gxid;
/// 关系名称
@property (copy, nonatomic) NSString *gxName;
/// 是否购买了亲子包
@property (copy, nonatomic) NSString *qzb_flag;
/// 权限
@property (copy, nonatomic) NSString *qx;
/// 是否主家长
@property (copy, nonatomic) NSString *isMainJiaZhang;
/// 发表动态数
@property (copy, nonatomic) NSString *fbdt_num;
/// 上传照片数
@property (copy, nonatomic) NSString *pic_num;
/// 剩余乐豆数
@property (copy, nonatomic) NSString *ledou_num;

- (id)initWithDic:(NSDictionary *)dic;
-(id)copyWithZone:(NSZone *)zone;

@end
