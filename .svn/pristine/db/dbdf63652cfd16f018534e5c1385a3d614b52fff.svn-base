#import "ClassModel.h"
#import "AppMacro.h"

@implementation ClassModel

- (instancetype)initWithDic:(NSDictionary *)dic {
  self = [super init];
  if (self) {
    self.classname = pickJsonStrValue(dic, @"classname");
    self.classuid = pickJsonStrValue(dic, @"classid");
    self.dtype = DYNA_TYPE_CLASS;
  }
  return self;
}

@end
